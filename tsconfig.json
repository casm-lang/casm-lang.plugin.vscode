{ "compilerOptions":
  { "target":                     "es6"
  , "module":                     "commonjs"
  , "outDir":                     "obj"
  , "lib":
    [ "es6"
    , "es2016"
    ]
  , "rootDir":                    "."
  , "sourceMap":                  true
  , "alwaysStrict":               true
  , "noEmitOnError":              true
  , "noFallthroughCasesInSwitch": true
  , "noImplicitAny":              true
  , "noImplicitReturns":          true
  , "noImplicitThis":             true
  }
, "exclude":
  [ "node_modules"
  ]
}
